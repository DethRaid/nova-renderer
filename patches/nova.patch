+    public FileResource(ResourceLocation location) {
+        this.location = location;
+    }
+
+    public ResourceLocation getResourceLocation() {
+        return location;
+    }
+
+    public InputStream getInputStream() {
+        String[] assetPaths = new String[] {
+                "assets",
